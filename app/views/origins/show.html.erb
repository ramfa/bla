<p id="notice"><%= notice %></p>

<p>
  <strong>Donador:</strong>
  <%= @origin.colector %>
</p>

<p>
  <strong>Dirección:</strong>
  <%= @origin.address %>
</p>

<p>
  <strong>Alimentación:</strong>
  <%= @origin.feeding %>
</p>

<p>
  <strong>Metodo de Captura:</strong>
  <%= @origin.capmethod %>
</p>

<p>
  <strong>Caracteristicas:</strong>
  <%= @origin.features %>
</p>

<p>
  <strong>Provincia:</strong>
  <% if @origin.province %>
  <%= @locality.name %>
  <% end %>
</p>

<%= link_to 'Edit', edit_origin_path(@origin) %> |
<%= link_to 'Ok', new_entry_path %>
